.image {
  height: 100%;
}

.tabs {
  display: flex;
  align-items: center;
  padding: 2rpx 25rpx;
  position: fixed;
  top: 0;
  width: 100%;
  z-index: 10;
  -webkit-backdrop-filter: blur(15px);
  backdrop-filter: blur(15px);
  background-color: #eee;
}

.tabs_item {
  font-size: 35rpx;
  height: 70rpx;
  padding: 0rpx 25rpx;
}

.active {
  border-bottom: 1px solid #ed4014;
  color: #ed4014;
}

.search {
  margin-left: 20rpx;
}

.nav {
  display: flex;
  padding: 25rpx 0;
  font-size: 26rpx;
  justify-content: center;
  border-bottom: 1rpx solid #eee;
  background-color: #fff;
}

.nav .nav_item {
  width: 33.3333333%;
  display: flex;
  flex-wrap: wrap;
  justify-content: center;
}

.nav .nav_item image {
  display: block;
  width: 110rpx;
  height: 110rpx;
  border: 1rpx solid #bb2c08;
  border-radius: 50%;
  margin: 20rpx 40rpx;
}

.thisday {
  font-size: 56rpx;
  color: #bb2c08;
  display: block;
  width: 110rpx;
  height: 110rpx;
  border: 1rpx solid #bb2c08;
  border-radius: 50%;
  margin: 20rpx 40rpx;
  line-height: 110rpx;
  text-align: center;
  font-weight: 300;

}

.recommend {
  font-size: 26rpx;
  background-color: #fff;
  align-content: center;
}

.recommend  .rec_nav {
  display: flex;
  justify-content: space-between;
  align-content: center;
  overflow: hidden;
  margin: 0 15rpx 10rpx 15rpx;
  padding: 10rpx;
  padding-bottom: 0;
  padding-left: 0;
  line-height: 36rpx;
  font-size: 28rpx;
}

.rec_nav  .rec_more {
  color: #9999;
  line-height: 14rpx;
}

.rec_img {
  width: 36rpx;
  height: 36rpx;
  margin-right: 20rpx;
}

.rec_content {
  display: flex;
  margin-right: 10rpx;
  flex-wrap: wrap;
  box-sizing: border-box;
}

.content_item {
  margin: 15rpx 0 15rpx 15rpx;
  box-sizing: border-box;
  width: 31.333333%;
  position: relative;
}

.content_item .modal {
  display: flex;
  position: absolute;
  right: 0;
  top: 0;
  color: #fff;
  z-index: 1;
  font-size: 24rpx;
  padding: 8rpx 15rpx 8rpx 40rpx;
  background-image: linear-gradient(90deg, rgba(0, 0, 0, 0), rgba(0, 0, 0, 0.2));
  max-width: 70%;
  text-align: right;
  min-width: 30%;
}

.content_item .modal image {
  width: 20rpx;
  height: 27rpx;
  margin-right: 13rpx;
}

.content_item image {
  width: 100%;
  height: 270rpx;
}

.rec_content .content_item .name {
  margin: auto;
  display: block;
  overflow: hidden;
  max-height: 98rpx;
  text-overflow: ellipsis;
  line-height: 32rpx;
}

.mv {
  display: flex;
  flex-wrap: wrap;
}

.mv .content_item {
  margin: 15rpx 0 15rpx 15rpx;
  box-sizing: border-box;
  width: 48%;
  position: relative;
}

.artistName {
  color: #888;
  font-size: 24rpx;
}

.mv  .content_item .name {
  margin: auto;
  display: block;
  overflow: hidden;
  max-height: 62rpx;
  text-overflow: ellipsis;
  line-height: 32rpx;
  word-break: keep-all;
}

/* 歌单 */

.song .cate {
  display: flex;
  justify-content: space-between;
  font-size: 33rpx;
  background-color: #fff;
  padding: 20rpx 20rpx;
}

.song .sort {
  border: 1px solid #9999;
  border-radius: 5rpx;
  padding: 0 5rpx;
}

.song .content {
  display: flex;
  flex-wrap: wrap;
  background-color: #fff;
}

.song .content_item {
  font-size: 28rpx;
  margin: 15rpx 0 15rpx 15rpx;
  box-sizing: border-box;
  width: 47%;
  position: relative;
}

.song .content_item .modal {
  display: flex;
  position: absolute;
  right: 0;
  top: 0;
  color: #fff;
  z-index: 1;
  font-size: 24rpx;
  padding: 8rpx 15rpx 8rpx 40rpx;
  background-image: linear-gradient(90deg, rgba(0, 0, 0, 0), rgba(0, 0, 0, 0.2));
  max-width: 70%;
  text-align: right;
  min-width: 30%;
}

.song .content_item .modal_bottom {
  display: flex;
  position: absolute;
  left: 0;
  bottom: 100rpx;
  color: #fff;
  z-index: 1;
  font-size: 24rpx;
  padding: 8rpx 15rpx 8rpx 40rpx;
  background-image: linear-gradient(90deg, rgba(0, 0, 0, 0), rgba(0, 0, 0, 0.2));
  max-width: 100%;
  text-align: right;
  min-width: 50%;
}

.song .content_item .modal image {
  width: 20rpx;
  height: 27rpx;
  margin-right: 13rpx;
}

.song .content_item .modal_bottom image {
  width: 20rpx;
  height: 27rpx;
  margin-right: 13rpx;
}

.content_item image {
  width: 100%;
  height: 270rpx;
}

.song .content_item .name {
  margin: auto;
  display: block;
  overflow: hidden;
  max-height: 98rpx;
  text-overflow: ellipsis;
  line-height: 32rpx;
}

.showCate {
  padding: 0 15rpx;
  box-sizing: border-box;
  position: fixed;
  width: 100%;
  height: 100%;
  z-index: 10;
  left: 0;
  top: 0;
  background-color: #fff;
  font-size: 30rpx;
}

.hide {
  border: 1px solid #9999;
  padding: 50rpx 50rpx;
  display: block;
  width: 100%;
  background-color: #8b6c65;
  text-align: center;
  margin-bottom: 20rpx;
}

.song_title {
  font-size: 35rpx;
  display: flex;
  justify-content: space-between;
  padding: 10rpx 20rpx;
  border-left: 2px solid #ed4014;
}

.song {
  padding-top: 91rpx;
  background-color: #fff;
}

.img_creator {
  position: absolute;
  right: 0;
  bottom: 68rpx;
  line-height: 32rpx;
  color: #fff;
  z-index: 2;
  width: 100%;
  padding: 40rpx 10rpx 10rpx;
  font-size: 24rpx;
  box-sizing: border-box;
  background-image: linear-gradient(rgba(255, 255, 255, 0), rgba(0, 0, 0, 0.2));
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
}

.img_creator image {
  width: 20rpx;
  height: 20rpx;
}

.showCate {
  padding: 0 15rpx;
}

.showCate .all {
  display: block;
  width: 98%;
  height: 85rpx;
  border: 1px solid #eee;
  line-height: 85rpx;
  text-align: center;
}

.showCate .cl_ico_hot {
  background-image: url(data:image/png;base64,);
  left: 0;
  top: 0;
  background-size: 56rpx 56rpx;
  background-repeat: no-repeat;
}

.showCate .checked {
  border: 1px solid #ed4014;
  color: #ed4014;
  background-repeat: no-repeat;
  background-image: url(data:image/png;base64,);
  /* width: 200rpx;
  height: 38rpx; */
  background-position: right bottom;
  right: 0;
  bottom: 0;
  background-size: 48rpx 48rpx;
}

.catelist {
  margin-bottom: 20rpx;
  overflow: hidden;
  padding-bottom: 2rpx;
  font-size: 26rpx;
}

.catelist .cl_ico {
  height: 182rpx;
  width: 182rpx;
  line-height: 1;
}

.cl_ico image {
  width: 80rpx;
  height: 80rpx;
  margin: 35rpx auto 15rpx;
  display: block;
}

.cl_list {
  width: 179rpx;
  border: 1rpx solid #eee;
  text-align: center;
  height: 90rpx;
  font-size: 26rpx;
  line-height: 90rpx;
  overflow: hidden;
  position: relative;
  z-index: 1;
  margin: 0 -1rpx -1rpx 0;
  float: left;
}

.hot {
  position: absolute;
  top: 0;
  left: 0;
  background-color: #ed4014;
  color: #fff;
  font-size: 20rpx;
  width: 20%;
  height: 30%;
}
