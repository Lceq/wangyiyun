<!-- 轮播图 -->
<view>
  <swiper autoplay="{{autoplay}}" interval="{{interval}}" duration="{{duration}}" circular indicator-dots>
    <block wx:for="{{imgs}}" wx:key="id">
      <swiper-item>
        <image src="{{item.url}}" class="image"></image>
      </swiper-item>
    </block>
  </swiper>
</view>
